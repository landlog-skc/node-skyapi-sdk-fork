{
  "name": "@skycatch/node-skyapi-sdk",
  "version": "0.0.1-alpha.2",
  "description": "Node SDK to interact with SkyAPI",
  "main": "./dist/skyapi.js",
  "scripts": {
    "build": "npm run build:sdk && npm run build:json && npm run build:yaml",
    "build:sdk": "node bin/build.js sdk",
    "build:json": "node bin/build.js json",
    "build:yaml": "node bin/build.js yaml",
    "test": "npm run build:sdk && npm run test:ci",
    "test:ci": "NODE_ENV=test tap ./test/**/*.spec.js --cov",
    "test:cov": "NODE_ENV=test tap ./test/**/*.spec.js --cov --coverage-report=html",
    "test:debug": "NODE_ENV=test DEBUG=@skycatch/* DEBUG_COLORS=true mocha --timeout 10000"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@zeit/fetch-retry": "^4.0.0",
    "debug": "^4.1.1",
    "js-beautify": "^1.10.0",
    "jws": "^3.2.2",
    "mustache": "^3.0.1",
    "node-fetch": "^2.4.1",
    "openapi-schema-validation": "^0.4.2",
    "qs": "^6.7.0",
    "swagger-parser": "^6.0.5"
  },
  "devDependencies": {
    "@skycatch/skyapi-datasets-post": "github:skycatch/skyapi-datasets-post",
    "@skycatch/skyapi-datasets-processes-post": "github:skycatch/skyapi-datasets-processes-post",
    "@skycatch/skyapi-designfiles-get": "github:skycatch/skyapi-designfiles-get#release/v1.0.0",
    "@skycatch/skyapi-geoids-list": "github:skycatch/skyapi-geoids-list",
    "@skycatch/skyapi-projections-get": "github:skycatch/skyapi-projections-get",
    "mocha": "^6.1.4",
    "tap": "^14.2.2"
  }
}
